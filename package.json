{
  "name": "karma-browserstack-example",
  "version": "1.0.0",
  "description": "Just another unit test example project to run Mocha test based on Karma on BrowserStack infrastructure using karma-browserstack-launcher",
  "main": "src/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test:local:unit": "./node_modules/.bin/karma start local.karma.conf.js --singleRun true --debug",
    "test:ci:unit": "./node_modules/.bin/karma start browserstack.karma.conf.js --singleRun true --debug"
  },
  "author": "Kyungwook Park",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-runtime": "^6.20.0",
    "babelify": "^7.3.0",
    "es3ify-loader": "^0.2.0",
    "karma": "^1.3.0",
    "karma-browserstack-launcher": "^1.1.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-safari-launcher": "^1.0.0",
    "karma-webpack": "^1.8.1",
    "standard": "^8.6.0",
    "standard-loader": "^5.0.0",
    "webpack": "^1.14.0"
  }
}
